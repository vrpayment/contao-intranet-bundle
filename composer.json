{
  "name": "vrpayment/contao-intranet-bundle",
  "type": "contao-bundle",
  "description": "Small bundle with nice functions for internal purposes",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "VrPayment",
      "homepage": "https://github.com/vrpayment"
    }
  ],
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "4.7.*",
    "symfony/framework-bundle": "^3.3",
    "symfony/monolog-bundle": "^3.4",
    "ext-curl": "*",
    "ramsey/uuid": "^3.8",
    "netzmacht/contao-form-bundle": "1.*"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "friendsofphp/php-cs-fixer": "^2.12",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message-factory": "^1.0.2",
    "phpunit/phpunit": "^5.7.26",
    "symfony/phpunit-bridge": "^3.2"
  },
  "extra": {
    "contao-manager-plugin": "Vrpayment\\ContaoIntranetBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Vrpayment\\ContaoIntranetBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vrpayment\\ContaoIntranetBundle\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/vrpayment/issues",
    "source": "https://github.com/vrpayment"
  }
}
